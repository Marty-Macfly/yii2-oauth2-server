{
  "name": "macfly/yii2-oauth2-server",
  "description": "Wrapper for filsh yii2-oauth2-server provide authorize, token, user controller",
  "keywords": ["yii", "extension", "module", "oauth", "oauth2"],
  "homepage": "https://github.com/macfly/yii2-oauth2-server",
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "source": "https://github.com/macfly/yii2-oauth2-server"
  },
  "authors": [
    {
      "name": "Charles DELFLY",
      "email": "c.delfly@linkbynet.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "2.0.12",
    "kartik-v/yii2-datecontrol": "*",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "filsh/yii2-oauth2-server": "2.0.1"
  },
  "autoload": {
    "psr-4": {
      "macfly\\oauth2server\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "macfly\\oauth2server\\Bootstrap"
  }
}
